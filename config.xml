<?xml version='1.0' encoding='utf-8'?>
<widget id="com.scan.QRcode" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>scanQRcodeApp</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

    <platform name="android">

    <!-- dÃ©but pref test HTTP/S CORS -->

    <allow-navigation href="*" />
    <preference name="scheme" value="http" />
    <preference name="hostname" value="localhost" />
    <preference name="android-insecure-http" value="true" />

    <!-- FIN -->

    <preference name="AndroidXEnabled" value="true" />
    <preference name="GradlePluginGoogleServicesEnabled" value="true" />
    </platform>

</widget>
